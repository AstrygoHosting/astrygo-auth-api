{
  "name": "astrygo-dashboard",
  "private": true,
  "workspaces": [
    "api/auth-api",
    "apps/web"
  ],
  "scripts": {
    "build": "npm run build:api && npm run build:web",
    "build:api": "npm --workspace api/auth-api run build",
    "build:web": "npm --workspace apps/web run build",
    "dev:api": "npm --workspace api/auth-api run dev",
    "dev:web": "npm --workspace apps/web run dev"
  }
}
